<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="404.0" prefWidth="284.0" xmlns="http://javafx.com/javafx/23" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="org.example.app.controllers.AddExistProductController">
    <Label layoutX="94.0" layoutY="14.0" text="Add exist Product"/>
    <TextField fx:id="quantityField" layoutX="62.0" layoutY="68.0" promptText="Quantity"/>
    <ComboBox fx:id="productNameComboBox" layoutX="60.0" layoutY="107.0" prefHeight="26.0" prefWidth="160.0"
              promptText="Product name"/>
    <Button fx:id="backButton" layoutX="35.0" layoutY="215.0" mnemonicParsing="false" prefHeight="26.0" prefWidth="44.0"
            text="↩️"/>
    <Button fx:id="saveButton" layoutX="188.0" layoutY="215.0" mnemonicParsing="false" prefHeight="26.0"
            prefWidth="50.0" text="Save"/>
</AnchorPane>
