<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.Scene?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<?import javafx.stage.Stage?>

<Stage xmlns="http://javafx.com/javafx/23" xmlns:fx="http://javafx.com/fxml/1">
    <scene>
        <Scene>
            <AnchorPane prefHeight="245.0" prefWidth="300.0">
                <children>
                    <Label layoutX="0" layoutY="10" prefWidth="300" text="Edit Menu" alignment="CENTER">
                        <font>
                            <Font name="Ayuthaya" size="16.0" />
                        </font>
                    </Label>
                    <Button fx:id="addProductButton" layoutX="84.0" layoutY="80.0" mnemonicParsing="false" text="Add Product" />
                    <Button fx:id="backToMainMenuButton" layoutX="84.0" layoutY="130.0" mnemonicParsing="false" text="Back to Main Menu" />
                </children>
            </AnchorPane>
        </Scene>
    </scene>
</Stage>